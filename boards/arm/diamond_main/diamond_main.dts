    // io-expander 0x21
    // PCA9535 registers are the same as one TCA9535
    gpio_exp1: pca95xx@21 {
        compatible = "nxp,pca95xx";
        reg = <0x21>;
        gpio-controller;
        #gpio-cells = <2>;
        ngpios = <16>;
